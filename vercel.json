{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "pages/api/trending.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/analyze/(.*)",
      "dest": "/src/index.js"
    },
    {
      "src": "/health",
      "dest": "/src/index.js"
    },
    {
      "src": "/config",
      "dest": "/src/index.js"
    },
    {
      "src": "/quick-test",
      "dest": "/src/index.js"
    },
    {
      "src": "/api/trending",
      "dest": "/pages/api/trending.js"
    },
    {
      "src": "/trending",
      "dest": "/pages/trending.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}